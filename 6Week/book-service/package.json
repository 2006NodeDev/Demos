{
  "name": "lightlyburning-book-service",
  "version": "1.0.0",
  "description": "Def not amazon kindle",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm install && tsc && npm prune --production && cp ./package.json ./build",
    "build:clean": " rm -rf ./build && npm install && tsc && npm prune --production && cp ./package.json ./build",
    "deploy": "node dist/index.js"
  },
  "author": "2006NodeDev",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/pubsub": "^2.3.0",
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.3",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
